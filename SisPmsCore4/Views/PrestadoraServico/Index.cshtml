                        @Html.ActionLink("Detalhes", "Details", new { id = item.idPrestadoraServico }, new { @class = "btn btn-secondary" }) |
                        @Html.ActionLink("Deletar", "Delete", new { id = item.idPrestadoraServico }, new { @class = "btn btn-danger" })
                    </td>
